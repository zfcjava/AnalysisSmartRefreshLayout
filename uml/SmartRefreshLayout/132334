      operation 211694 "onReleased"
	public explicit_return_type "void"
	nparams 3
	  param in name "refreshLayout" type class_ref 132462 // RefreshLayout
	  param in name "height" explicit_type "int"
	  param in name "maxDragHeight" explicit_type "int"
	
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}@NonNull final ${t0} ${p0}, ${t1} ${p1}, ${t2} ${p2}${)}${throws}${staticnl}{
  ${body}}
"
	java_annotation "@Override
"
	
	
	
	comment "    @Override
    public void onPulling(float percent, int offset, int height, int maxDragHeight) {
        mWavePulling = true;
        mWaveTop = Math.min(height, offset);
        mWaveHeight = (int) (1.9f * Math.max(0, offset - height));
        mDotFraction = percent;
    }

    @Override
    public void onReleasing(float percent, int offset, int height, int maxDragHeight) {
        if (mWavePulling) {
            onPulling(percent, offset, height, maxDragHeight);
        }
    }"
      end

